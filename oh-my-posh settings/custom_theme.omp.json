{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          },
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "<transparent,#ff4500>\ue0b2</>",
          "template": " {{if .WSL }}WSL at {{end}}{{.Icon}}",
          "foreground": "#ffffff",
          "background": "#ff4500",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#FFFB38>\ue0b2</>",
          "template": "\ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ end }}",
          "foreground": "#000000",
          "background": "#FFFB38",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "ÓÇ¥</>",
          "template": "\ue266 MEM: {{ round .PhysicalPercentUsed .Precision }}%|{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB üñ•Ô∏è",
          "foreground": "#ffffff",
          "background": "#516BEB",
          "type": "sysinfo",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#17D7A0>\ue0b2</>",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#011627",
          "background": "#17D7A0",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#d6deeb",
          "background": "#575656",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2500\u256e",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u256d\u2500",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf292 ",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "Óóæ ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "Óòó "
          },
          "template": " {{ .Path }} ",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "template": " {{ .CurrentDate | date .Format  }}",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2500\u256f",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2570\u2500 ",
          "type": "status",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
